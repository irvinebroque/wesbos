  "eslintConfigxx": {
    "extends": [
      "wesbos/typescript"
    ],
    "rules": {
      "react/prop-types": 0,
      "import/no-unresolved": 0,
      "//Need to turn this off while we upgrade to ESLint 9. Something weird with the css module approach": 0,
      "@typescript-eslint/no-unsafe-assignment": 0
    }
  },
  "scripts": {
    "lint": "eslint --ext .js,.jsx --ignore-pattern public .",
    "lint:fix": "eslint --ext .js,.jsx --ignore-pattern public . --fix",
    "test": "echo \"Error: no test specified\" && exit 1",
    "develop": "gatsby develop",
    "build-gatsby": "gatsby build",
    "netlify": "netlify dev",
    "deploy": "gatsby-parallel-runner --prefix-paths && gh-pages -d public",
    "xxprebuild": "netlify-lambda install",
    "scrape": "NODE_OPTIONS='--inspect=localhost:4444' node scrape-tips.js",
    "dev": "next dev --turbo",
    "build": "next build",
    "start": "next start",
    "build:worker": "pnpm db:migrate-local-db && opennextjs-cloudflare",
    "dev:worker": "wrangler dev --port 8771",
    "preview:worker": "pnpm run build:worker && pnpm run dev:worker",
    "deploy:worker": "pnpm run build:worker && wrangler deploy",
    "types:worker": "wrangler types --experimental-include-runtime",
    "db:migrate": "drizzle-kit migrate",
    "db:studio": "drizzle-kit studio",
    "db:generate": "drizzle-kit generate",
    "db:push": "drizzle-kit push",
    "db:migrate-local-db": "wrangler d1 migrations apply wes-bos-social-cache --local",
    "tip": "tsx --conditions=react-server ./createTip.ts"
  },
  "packageManager": "pnpm@9.10.0+sha1.216899f511c8dfde183c7cb50b69009c779534a8"
